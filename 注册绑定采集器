class WorkerSignals(QObject):  # ÂÆö‰πâ‰ø°Âè∑Á±ªÔºåÁî®‰∫éÁ∫øÁ®ãÈó¥ÈÄö‰ø°
    finished = pyqtSignal(str)


class DataWorker(QRunnable):  # Êï∞ÊçÆÂ§ÑÁêÜÁ∫øÁ®ãÔºöÊåâÊó∂Èó¥ËåÉÂõ¥Ëé∑ÂèñÂ∑≤ÁªëÂÆöÊâãÊú∫Âè∑
    def __init__(self, signals, port, token, code, start_time, end_time):
        super().__init__()
        self.signals = signals
        self.port = port
        self.code = code
        self.token = token
        self.start_time = start_time
        self.end_time = end_time

    def run(self):
        headers = {
            "Cookie": f"token={self.token}",
        }

        # -------- Â∑•ÂÖ∑ÂáΩÊï∞ÔºöÂ∏¶ÈáçËØïÁöÑ POST ËØ∑Ê±Ç --------
        RETRY_STATUS = {429, 500, 502, 503, 504}

        def make_request(url, data, headers, max_retries=5, base_delay=1, timeout=10):
            last_exception = None
            empty_retry_used = False

            for attempt in range(1, max_retries + 1):
                try:
                    resp = requests.post(url, data=data, headers=headers, timeout=timeout)

                    if not resp.ok:
                        if resp.status_code in RETRY_STATUS:
                            raise requests.HTTPError(f"HTTP {resp.status_code} ÂèØÈáçËØï")
                        else:
                            raise requests.HTTPError(f"HTTP {resp.status_code} ‰∏çÈáçËØï")

                    try:
                        j = resp.json()
                    except Exception as je:
                        raise ValueError(f"JSON Ëß£ÊûêÂ§±Ë¥•: {je}")

                    if j.get("code", 0) != 0:
                        raise ValueError(f"‰∏öÂä°ÈîôËØØ code={j.get('code')} msg={j.get('msg', '')}")

                    result = j.get("result", [])

                    if result:
                        return result

                    if not empty_retry_used:
                        empty_retry_used = True
                        time.sleep(0.3)
                        continue
                    else:
                        return []

                except (requests.RequestException, ValueError) as e:
                    last_exception = e
                    if attempt == 1:
                        time.sleep(0.3)
                    delay = base_delay * (2 ** (attempt - 1)) + random.uniform(0.5, 1.5)
                    time.sleep(min(delay, 10))

            print(f"[Â§±Ë¥•] ÈáçËØï {max_retries} Ê¨°‰ªçÂ§±Ë¥•: {url}")
            if last_exception:
                print(f"[ÊúÄÁªàÈîôËØØ] {last_exception}")
            return []

        # -------- ‰∏öÂä°ÈÄªËæëÔºà‰øÆÂ§çÂàÜÈ°µ‰∏çÂÆåÊï¥Ôºâ--------
        base_url = f"http://v.boinht.com:{self.port}/user/pf_list_with_game"
        page = 1
        # ÁúüÂÆûÊØèÈ°µ‰∏äÈôêÂæàÂèØËÉΩÊòØ 30ÔºàÊúçÂä°Á´ØÂº∫Âà∂ÔºâÔºåËøôÈáåÂÜô 30 Êõ¥Áõ¥ËßÇÔºõÂÜô 1000 ‰πüÊ≤°Áî®
        page_size = 32

        lines = []
        seen_ids = set()      # ÂéªÈáçÔºöÊúâÁöÑÊé•Âè£Ë∂ÖÈ°µÂêé‰ºöÈáçÂ§çËøîÂõûÊúÄÂêé‰∏ÄÈ°µ
        max_pages = 100000    # Èò≤Ê≠¢ÂºÇÂ∏∏ÂØºËá¥Ê≠ªÂæ™ÁéØ

        while page <= max_pages:
            payload = {
                "channel_id": "",
                "open_channel_id": "",
                "id": "",
                "game_user_id": "",
                "is_bind": "1",
                "is_black": "",
                "nickname": "",
                "is_online": "",
                "reg_ip": "",
                "is_recharge": "",
                "device_type": "",
                "mobile": "",
                "apns_code": "",
                "begin_time": self.start_time,
                "end_time": self.end_time,
                "page_size": page_size,
                "page": page,
                "login_code": self.code,
            }

            result = make_request(base_url, payload, headers=headers)

            # ÂÖ≥ÈîÆÔºö‰ª•‚ÄúÁ©∫È°µ‚Äù‰∏∫ÁªìÊùüÊù°‰ª∂ÔºàËÄå‰∏çÊòØ len(result) < page_sizeÔºâ
            if not result:
                if page == 1 and not lines:
                    print("Ê≤°ÊúâÊï∞ÊçÆ")
                break

            new_count = 0
            for aa in result:
                rid = aa.get("id")  # ‰Ωú‰∏∫ÂéªÈáç‰∏ªÈîÆ
                if rid and rid in seen_ids:
                    continue
                if rid:
                    seen_ids.add(rid)

                appid = rid or ""
                merchant = aa.get("channel_id", "")
                sub_channel = aa.get("open_channel_id", "")
                mobile = aa.get("mobile", "")

                # ÂéªÊéâÊâãÊú∫Âè∑ÂâçÁºÄ86
                if mobile.startswith('86') and len(mobile) > 2:
                    mobile = mobile[2:]

                line = f"APPID: {appid}, ÂïÜÊà∑: {merchant}, Â≠êÊ∏†ÈÅì: {sub_channel}, ÁîµËØùÂè∑Á†Å: {mobile}"
                print(line)
                lines.append(line)
                new_count += 1

            # ‰∏ã‰∏ÄÈ°µ
            page += 1
            # ËΩªÂæÆËäÇÊµÅÔºåÈÅøÂÖç 429
            time.sleep(0.2)

        output = "\n".join(lines) if lines else "Ê≤°ÊúâÊï∞ÊçÆ"
        self.signals.finished.emit(output)


class UserWindow(QWidget):  # ‰∏ªÁ™óÂè£Á±ª
    def __init__(self):
        super().__init__()
        # Âä†ËΩΩ base64 ÂõæÊ†áÔºàÁ°Æ‰øùÂ≠óÁ¨¶‰∏≤ÂÆåÊï¥Ôºâ
        icon_base64 = ""  # ÊõøÊç¢Êàê‰Ω†ÁöÑ base64
        icon_data = QByteArray.fromBase64(icon_base64.encode())

        # Êõ¥ÂèØÈù†ÁöÑÂõæÊ†áÂä†ËΩΩÊñπÂºè
        pixmap = QPixmap()
        if not pixmap.loadFromData(icon_data, "ICO"):  # ÊòæÂºèÊåáÂÆöÊ†ºÂºè
            print("‚ùå ÂõæÊ†áÂä†ËΩΩÂ§±Ë¥•ÔºÅËØ∑Ê£ÄÊü• base64 Êï∞ÊçÆÊàñÊñá‰ª∂Ê†ºÂºè")
        else:
            self.setWindowIcon(QIcon(pixmap))  # ËÆæÁΩÆÁ™óÂè£Âíå‰ªªÂä°Ê†èÂõæÊ†á

        self.setWindowTitle("Ê≥®ÂÜåÁªëÂÆöÈááÈõÜÂô®")
        self.resize(1200, 800)  # ÂàùÂßãÂ§ßÂ∞è
        #  self.setMinimumSize(800, 500)  # ÈôêÂà∂ÊúÄÂ∞è

