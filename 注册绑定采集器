        col.addSpacing(15)

        # ÁâàÊú¨Ë°åÔºàÂ≠óÂè∑Âä†Â§ßÔºâ
        ver_row = QHBoxLayout()
        ver_row.setSpacing(8)

        ver_label = QLabel("ÁâàÊú¨Ôºö")
        ver_label.setStyleSheet("font-size:16px;")  # ‚Üê Âä†Â§ß

        ver_value = QLabel(version)
        ver_value.setStyleSheet("font-size:16px; color:#0f766e; font-weight:600;")  # ‚Üê Âä†Â§ß

        date_label = QLabel(f"  Êõ¥Êñ∞Êó•ÊúüÔºö{update_date}")
        date_label.setStyleSheet("font-size:16px; color:#64748b;")  # ‚Üê Âä†Â§ß

        ver_row.addWidget(ver_label, 0, Qt.AlignLeft)
        ver_row.addWidget(ver_value, 0, Qt.AlignLeft)
        ver_row.addWidget(date_label, 0, Qt.AlignLeft)
        ver_row.addStretch(1)
        col.addLayout(ver_row)
        col.addSpacing(35)

        # ‚ÄúÂ£∞ÊòéÔºö‚Äù Ê†áÈ¢ò
        decl_title = QLabel("Â£∞ÊòéÔºö")
        decl_title.setStyleSheet("font-size:14px; font-weight:700; color:#111827;")
        col.addWidget(decl_title, 0, Qt.AlignLeft)
        col.addSpacing(12)

        # ÁôΩËâ≤Ê°Ü
        box = QFrame()
        box.setStyleSheet("""
            QFrame {
                background: #ffffff;
                border-radius: 8px;
            }
        """)
        box.setSizePolicy(QSizePolicy.Expanding, QSizePolicy.Expanding)
        box_layout = QVBoxLayout(box)
        box_layout.setContentsMargins(14, 12, 14, 12)
        box_layout.setSpacing(6)

        # Â£∞ÊòéÊ≠£ÊñáÔºàÊúÄÂêé‰∏ÄÂè•Âä†Á≤óÔºâ
        body = QLabel(
            "<div style='color:#374151; font-size:15px; line-height:1.9;'>"
            "Êú¨ËΩØ‰ª∂‰ªÖ‰æõÂÖ∑Â§áÂêéÂè∞ËÆøÈóÆÊùÉÈôê‰∏îÂ∑≤Ëé∑ÊéàÊùÉÁöÑÁî®Êà∑Âú®ÂêàËßÑÂâçÊèê‰∏ã‰ΩøÁî®,‰∏ªË¶ÅÁî®‰∫éÈááÈõÜÂ∑≤Ê≥®ÂÜåÂπ∂ÁªëÂÆöÁöÑÁî®Êà∑Êï∞ÊçÆ„ÄÇ"
            "‰ΩøÁî®ËÄÖÂ∫îÁ°ÆËÆ§/‰øùËØÅÊï∞ÊçÆ‰∏éË¥¶Âè∑Êù•Ê∫êÂêàËßÑ„ÄÅÊéàÊùÉÂÖÖÂàÜÔºåÂπ∂ÂØπËá™Ë∫´Êìç‰ΩúÂèäÁªìÊûúÊâøÊãÖÂÖ®ÈÉ®Ë¥£‰ªª„ÄÇ<br/><br/>"
            "Âõ†ÊéàÊùÉ‰∏çË∂≥ÊàñËøùËßÑ‰ΩøÁî®ÂØºËá¥ÁöÑ‰ªª‰ΩïÊçüÂ§±„ÄÅÂ§ÑÁΩöÁ≠âÁî±‰ΩøÁî®ËÄÖËá™Ë°åÊâøÊãÖ„ÄÇÊú¨ËΩØ‰ª∂Êåâ‚ÄúÁé∞Áä∂‚ÄùÊèê‰æõÔºå‰∏ç‰Ωú‰ªª‰ΩïÊòéÁ§∫ÊàñÈªòÁ§∫ÊãÖ‰øùÔºõ"
            "‰ΩúËÄÖÂØπÂõ†‰ΩøÁî®ÊàñÊó†Ê≥ï‰ΩøÁî®Êú¨ËΩØ‰ª∂ÈÄ†ÊàêÁöÑ‰ªª‰ΩïÁõ¥Êé•ÊàñÈó¥Êé•ÊçüÂ§±‰∏çÊâøÊãÖË¥£‰ªª„ÄÇÊú¨ËΩØ‰ª∂‰∏éÁõ∏ÂÖ≥Âπ≥Âè∞/ÂïÜÊà∑/ÊîØ‰ªòÊú∫ÊûÑÊó†Èö∂Â±ûÊàñÂêà‰ΩúÂÖ≥Á≥ª„ÄÇ<br/><br/>"
            "<b>ÁªßÁª≠‰ΩøÁî®Âç≥Ë°®Á§∫ÊÇ®Â∑≤ÈòÖËØª„ÄÅÁêÜËß£Âπ∂ÂêåÊÑèÊú¨Â£∞Êòé„ÄÇ</b>"
            "</div>"
        )
        body.setWordWrap(True)
        body.setTextFormat(Qt.RichText)
        body.setContentsMargins(0, 0, 0, 0)
        body.setMargin(0)
        box_layout.addWidget(body, 0, Qt.AlignTop)
        box_layout.addStretch(1)

        col.addWidget(box, 1)

        layout.addWidget(content, 1)
        self.about_tab.setLayout(layout)

    def set_default_dates(self):  # ËÆæÁΩÆÈªòËÆ§Êó•Êúü‰∏∫‰ªäÂ§©ÂíåÊòéÂ§©
        current_date = datetime.now().date()
        tomorrow = current_date + timedelta(days=1)
        self.start_time_input.setDateTime(QDateTime(current_date.year, current_date.month, current_date.day, 0, 0))
        self.end_time_input.setDateTime(QDateTime(tomorrow.year, tomorrow.month, tomorrow.day, 0, 0))

    def move_to_previous_day(self):  # ÁßªÂä®Âà∞Ââç‰∏ÄÂ§©
        date = self.start_time_input.dateTime().addDays(-1)
        self.start_time_input.setDateTime(date)
        self.end_time_input.setDateTime(date.addDays(1))

    def move_to_next_day(self):  # ÁßªÂä®Âà∞Âêé‰∏ÄÂ§©
        date = self.start_time_input.dateTime().addDays(1)
        self.start_time_input.setDateTime(date)
        self.end_time_input.setDateTime(date.addDays(1))

    def start_processing(self):
        # ËÆæÁΩÆÂÖÅËÆ∏‰ΩøÁî®ÁöÑÊà™Ê≠¢Êó∂Èó¥ (YYYY-MM-DD HH:MM:SS)
        self.expiration_date_str = "2030-07-07 23:59:59"  # ËΩØ‰ª∂ËøáÊúüÊó∂Èó¥
        self.expiration_date = datetime.strptime(self.expiration_date_str, "%Y-%m-%d %H:%M:%S")

        # Ê£ÄÊü•ÊòØÂê¶ÈúÄË¶ÅÊèêÈÜí
        remaining_days = self.get_remaining_days()
        if remaining_days > 0 and remaining_days <= 7:
            QMessageBox.warning(self, "ËΩØ‰ª∂Âç≥Â∞ÜÊõ¥Êñ∞", f"Êú¨ËΩØ‰ª∂Â∞ÜÂú®{remaining_days}Â§©ÂêéËá™Âä®Êõ¥Êñ∞Ôºö\nËØ∑ËÅîÁ≥ª„ÄäÁÆ°ÁêÜÂëò„ÄãËé∑ÂèñÊúÄÊñ∞ÁâàÊú¨„ÄÇ")

        # Ê£ÄÊü•ÊòØÂê¶ËøáÊúü
        if self.is_expired():
            QMessageBox.critical(self, "ËΩØ‰ª∂Â∑≤Êõ¥Êñ∞", f"‰Ω†ÁöÑÂ∫îÁî®ÁâàÊú¨Ëøá‰ΩéÔºö\nËØ∑ËÅîÁ≥ª„ÄäÁÆ°ÁêÜÂëò„ÄãËé∑ÂèñÊúÄÊñ∞ÁâàÊú¨„ÄÇ")
            sys.exit(0)  # ÈÄÄÂá∫Á®ãÂ∫è

        self.finished_all = False
        # ‚Äî‚Äî Âü∫Á°ÄÊ†°È™åÔºà‰∏çÂÜçË¶ÅÊ±Ç user_ids Êñá‰ª∂Ôºâ‚Äî‚Äî
        port = self.port_input.text().strip()
        code = self.code_input.text().strip()

        missing_fields = []
        if not port:
            missing_fields.append("Á´ØÂè£ÔºàportÔºâ")
        if not code:
            missing_fields.append("È™åËØÅÁ†ÅÔºàcodeÔºâ")

        # Êî∂ÈõÜ‚ÄúÂ∑≤Â°´ÂÜô Cookie‚ÄùÁöÑÂ∞èÁªÑ
        group_jobs = []
        for group, token_input in self.token_inputs.items():
            token = token_input.text().strip() if token_input else ""
            if token:
                group_jobs.append((group, token))
        if not group_jobs:
            missing_fields.append("Ëá≥Â∞ë‰∏Ä‰∏™Â∞èÁªÑÁöÑ Cookie")

        if missing_fields:
            QMessageBox.warning(self, "ËæìÂÖ•ÈîôËØØ", "ËØ∑Â°´ÂÜô‰ª•‰∏ãÂÜÖÂÆπÔºö\n" + "\n".join(set(missing_fields)))
            return

        # ‚Äî‚Äî Êó∂Èó¥Ê†°È™å‰∏éËΩ¨Êç¢ ‚Äî‚Äî
        start = self.start_time_input.dateTime().toString("yyyy-MM-dd HH:mm:ss")
        end = self.end_time_input.dateTime().toString("yyyy-MM-dd HH:mm:ss")
        if end < start:
            QMessageBox.warning(self, 'Ë≠¶Âëä', 'ÁªìÊùüÊó∂Èó¥‰∏çËÉΩÊó©‰∫éÂºÄÂßãÊó∂Èó¥')
            return

        self.start_timestamp = int(datetime.strptime(start, '%Y-%m-%d %H:%M:%S').timestamp())
        self.end_timestamp = int(datetime.strptime(end, '%Y-%m-%d %H:%M:%S').timestamp())
        print(f"ÂºÄÂßãÊó∂Èó¥Êà≥: {self.start_timestamp}, ÁªìÊùüÊó∂Èó¥Êà≥: {self.end_timestamp}")
        self.port = port
        self.code = code

        self.group_order = [g for g, _ in group_jobs]

        # ‚Äî‚Äî ‰ªªÂä°/ËøõÂ∫¶ÂàùÂßãÂåñÔºàÊåâÂ∞èÁªÑÁªüËÆ°Ôºâ‚Äî‚Äî
        self.completed_tasks = []  # [(group, result_str), ...]
        self.group_jobs = group_jobs[:]  # ÂæÖÊ¥æÂèëÈòüÂàó
        self.total_jobs = len(group_jobs)  # ÊÄª‰ªªÂä°=Â∞èÁªÑÊï∞
        self.active_workers = 0

        # ‚Äî‚Äî Ëá™Âä®ÂØºÂá∫ÔºöÊØèÊ¨°ÂºÄÂßãÂâçÊ∏ÖÁ©∫ËÆ∞ÂΩï & ËÆæÂÆöÁõÆÂΩï ‚Äî‚Äî
        self.auto_exported_files = []
        self.auto_export_dir = os.getcwd()
        self.auto_export_ts = None
        # ÂØºÂá∫ÊåâÈíÆÊñáÂ≠óÂ§ç‰Ωç
        self.export_button.setText("ÂØºÂá∫Êï∞ÊçÆ")

        self.status_label.setText(f"Áä∂ÊÄÅÔºöÊ≠£Âú®Ëé∑ÂèñÊï∞ÊçÆ (0/{self.total_jobs})")
        self.progress_bar.setMaximum(self.total_jobs)
        self.progress_bar.setValue(0)
        self.progress_bar.setVisible(True)

        # ‰∏ÄÊ¨°ÊÄßÁ¶ÅÁî®Áõ∏ÂÖ≥ÊåâÈíÆ
        self.start_time_input.setEnabled(False)
        self.end_time_input.setEnabled(False)
        self.get_button.setEnabled(False)
        self.export_button.setEnabled(False)
        self.prev_day_btn.setEnabled(False)
        self.today_btn.setEnabled(False)
        self.next_day_btn.setEnabled(False)

        # ‚Äî‚Äî Á∫øÁ®ãÊ±† & Âπ∂Âèë ‚Äî‚Äî Êîπ‰∏∫È°∫Â∫èÊâßË°å
        if not hasattr(self, 'thread_pool'):
            self.thread_pool = QThreadPool()
        self.max_concurrency = 1  # È°∫Â∫èÔºö‰∏ÄÊ¨°Âè™Ë∑ë‰∏Ä‰∏™Â∞èÁªÑ
        self.thread_pool.setMaxThreadCount(self.max_concurrency)

        # ÂêØÂä®Ê¥æÂèë
        self._dispatch_more()

    def _dispatch_more(self):
        if getattr(self, "finished_all", False):
            return
        if (self.active_workers >= self.max_concurrency) or (not self.group_jobs):
            return

        group, token = self.group_jobs.pop(0)

        progress = len(self.completed_tasks)
        total = self.total_jobs
        self.status_label.setText(f"{progress}/{total} Â∑≤ÂÆåÊàê ‚Üí ÂΩìÂâçÂ∞èÁªÑÔºö{group}")

        signals = WorkerSignals()
        # Âº∫Âà∂‰∏ªÁ∫øÁ®ãÊâßË°å task_finishedÔºåÈÅøÂÖçË∑®Á∫øÁ®ãÈóÆÈ¢ò
        signals.finished.connect(lambda result, g=group: self.task_finished(g, result),
                                 Qt.QueuedConnection)

        worker = DataWorker(
            signals=signals,
            port=self.port,
            token=token,
            code=self.code,
            start_time=self.start_timestamp,
            end_time=self.end_timestamp,
        )

        self.thread_pool.start(worker)
        self.active_workers += 1

    def _restore_buttons(self):
        self.start_time_input.setEnabled(True)
        self.end_time_input.setEnabled(True)
        self.get_button.setEnabled(True)
        self.export_button.setEnabled(True)
        self.prev_day_btn.setEnabled(True)
        self.today_btn.setEnabled(True)
        self.next_day_btn.setEnabled(True)

    def task_finished(self, group, result):
        """Âçï‰∏™Â∞èÁªÑ‰ªªÂä°ÂÆåÊàêÂõûË∞ÉÔºöÂÆåÊàêÂêéÈöèÊú∫Á≠â 1~3 ÁßíÂÜçË∑ë‰∏ã‰∏Ä‰∏™"""
        if getattr(self, "finished_all", False):
            return  # Â∑≤ÁªèÂÆ£ÂëäÂÆåÊàê‰∫ÜÔºåÂ∞±Âà´Â§ÑÁêÜÂêéÁª≠‰ø°Âè∑

        self.completed_tasks.append((group, result))
        self.active_workers = max(0, self.active_workers - 1)

        progress = len(self.completed_tasks)
        total = self.total_jobs
        self.progress_bar.setValue(progress)

        # ÂÖ®ÈÉ®ÂÆåÊàê
        if (progress == total) and (not self.group_jobs) and (self.active_workers == 0):
            self.finished_all = True
            self.status_label.setText("Áä∂ÊÄÅÔºöÊâÄÊúâÂ∞èÁªÑÊï∞ÊçÆÂ∑≤Ëé∑ÂèñÂÆåÊàêÔºÅ")
            self.progress_bar.setVisible(False)

            # ‚ë† ÂÖàËá™Âä®ÂØºÂá∫ÔºàÊîæÂà∞‰∏ªÁ∫øÁ®ã‰∏ã‰∏ÄÂ∏ßÊâßË°åÔºåÈÅøÂÖçÈòªÂ°ûÂΩìÂâç‰ø°Âè∑ÂõûË∞ÉÔºâ
            QTimer.singleShot(0, self._auto_save_excels)

            # ‚ë¢ ÊèêÁ§∫Âπ∂ÊÅ¢Â§çÊåâÈíÆ
            QTimer.singleShot(0, lambda: QMessageBox.information(
                self, "ÊèêÁ§∫",
                "ÊâÄÊúâÂ∞èÁªÑÊï∞ÊçÆÂ∑≤Ëé∑ÂèñÂÆåÊàêÔºÅ\nÂ∑≤Ëá™Âä®Âú®Á®ãÂ∫èÁõÆÂΩïÁîüÊàê Excel Êñá‰ª∂„ÄÇ"
            ))
            QTimer.singleShot(0, self._restore_buttons)

            # ‰øùÈô©ÔºöÊ∏ÖÁêÜÁ∫øÁ®ãÊ±†ÈáåÂèØËÉΩÂ∞öÊú™Âá∫ÈòüÁöÑ‰ªªÂä°ÔºàQt 5.15+ ÊîØÊåÅ clearÔºõ‰∏çÊîØÊåÅÂèØÂøΩÁï•Ôºâ
            try:
                self.thread_pool.clear()
            except Exception:
                pass
            return

        # ËøòÊ≤°ÂÖ®ÈÉ®ÂÆåÊàêÔºöÈöèÊú∫Á≠âÂæÖ 1~3 ÁßíÂÜçÊ¥æÂèë‰∏ã‰∏Ä‰∏™
        import random
        pause = random.randint(1, 3)
        self.status_label.setText(
            f"{progress}/{total} Â∑≤ÂÆåÊàê ‚Üí Â∞èÁªÑ„Äê{group}„ÄëÂÆåÊàêÔºåÁ≠âÂæÖ {pause} ÁßíÂêéÁªßÁª≠‚Ä¶"
        )
        # Ëã•ÊúüÈó¥Ë¢´Ê†áËÆ∞‰∏∫ finished_allÔºåÂàô _dispatch_more ‰ºöÁ´ãÂç≥ËøîÂõû
        QTimer.singleShot(pause * 1000, self._dispatch_more)

    def _auto_save_excels(self):
        """
        Âú®ÂÖ®ÈÉ®Â∞èÁªÑËé∑ÂèñÂÆåÊàêÂêéËá™Âä®Ë∞ÉÁî®Ôºö
        1) ‰ªé self.completed_tasks ËÅöÂêàÊï∞ÊçÆÔºõ
        2) ÊåâÂ∞èÁªÑÂêÑÂÜô‰∏Ä‰∏™ Excel Âà∞ËΩØ‰ª∂ÁõÆÂΩïÔºõ
        3) ËÆ∞ÂΩï self.auto_exported_filesÔºå‰æõ‚ÄúÂØºÂá∫/ÊâìÂºÄË°®Ê†º‚ÄùÊåâÈíÆÁõ¥Êé•ÊâìÂºÄ„ÄÇ
        """
        try:
            from collections import defaultdict
            from openpyxl import Workbook
            from openpyxl.utils import get_column_letter
            from openpyxl.styles import Alignment
            import re, os, sys
            from datetime import datetime, timedelta

            # ------- ‰∏é export_data ‰øùÊåÅ‰∏ÄËá¥ÁöÑÂ∑•ÂÖ∑ÂáΩÊï∞ -------
            def parse_line(line: str):
                line = (line or "").strip()
                if not line:
                    return None
                parts = [p.strip() for p in line.split(",") if p.strip()]
                kv = {}
                for p in parts:
                    if "Ôºö" in p:
                        k, v = p.split("Ôºö", 1)
                    elif ": " in p:
                        k, v = p.split(": ", 1)
                    elif ":" in p:
                        k, v = p.split(":", 1)
                    else:
                        continue
                    kv[k.strip()] = v.strip()
                appid = kv.get("APPID", "")
                merchant = kv.get("ÂïÜÊà∑", "")
                sub_channel = kv.get("Â≠êÊ∏†ÈÅì", "")
                mobile = kv.get("ÁîµËØùÂè∑Á†Å", "")
                if not (appid or merchant or sub_channel or mobile):
                    return None
                return [appid, merchant, sub_channel, mobile]

            def safe_name(name: str, fallback="Êú™ÂëΩÂêç"):
                name = (name or "").strip()
                name = re.sub(r'[:\\/\?\*\[\]<>|" ]+', "_", name).rstrip("'")
                return name[:50] or fallback

            def to_appid(val: str):
                s = (val or "").strip()
                return int(s) if s.isdigit() else s

            def to_mobile(val: str):
                s = (val or "").strip()
                digits = re.sub(r"\D", "", s)
                if not digits:
                    return s
                if s.startswith("+") or s.startswith("0") or len(digits) > 15:
                    return s
                return int(digits) if digits.isdigit() and len(digits) <= 15 else s

            def to_sub_channel(val: str):
                s = (val or "").strip()
                if s.isdigit() and len(s) <= 15:
                    return int(s)
                return s

            header = ["ID", "ÂïÜÊà∑", "Â≠êÊ∏†ÈÅì", "ÊâãÊú∫"]

            # ------- ËÅöÂêàË°åÊï∞ÊçÆ -------
            group_rows = defaultdict(list)
            all_groups = set()
            for group, result in getattr(self, "completed_tasks", []):
                all_groups.add(group)
                if not result or str(result).strip() == "Ê≤°ÊúâÊï∞ÊçÆ":
                    continue
                for line in str(result).splitlines():
                    row = parse_line(line)
                    if row:
                        row[0] = to_appid(row[0])
                        row[2] = to_sub_channel(row[2])
                        row[3] = to_mobile(row[3])
                        group_rows[group].append(row)

            # Á°Æ‰øùÁ©∫ÁªÑ‰πü‰ºöÂØºÂá∫Á©∫Êñá‰ª∂
            for g in all_groups:
                group_rows.setdefault(g, [])

            # ------- ÂÜôÊñá‰ª∂ -------
            exported_files = []
            ts = (datetime.now() - timedelta(days=1)).strftime('%m-%d')  # ‰∏éÂéüÈÄªËæë‰∏ÄËá¥ÔºöÊò®Â§©
            out_dir = getattr(self, "auto_export_dir", os.getcwd())

            exported = 0
            for group in sorted(group_rows.keys(), key=lambda x: str(x)):
                rows = group_rows[group]
                wb = Workbook()
                ws = wb.active
                ws.title = "ÊòéÁªÜ"
                ws.append(header)
                for r in rows:
                    ws.append(r)

                # ÂàóÂÆΩ‰∏éÂ±Ö‰∏≠
                for col in range(1, ws.max_column + 1):
                    col_letter = get_column_letter(col)
                    ws.column_dimensions[col_letter].width = 12
                    for rr in range(1, ws.max_row + 1):
                        cell = ws.cell(row=rr, column=col)
                        cell.alignment = Alignment(horizontal='center', vertical='center')

                fname = f"{ts}_{safe_name(str(group), 'Êú™ÂëΩÂêçÁªÑ')}ÁªëÂÆöÂè∑Á†Å.xlsx"
                fpath = os.path.join(out_dir, fname)
                wb.save(fpath)
                exported_files.append(fpath)
                exported += 1

            # ËÆ∞ÂΩïÁªìÊûúÔºö‰æõ‚ÄúÂØºÂá∫/ÊâìÂºÄË°®Ê†º‚ÄùÊåâÈíÆÁõ¥Êé•ÊâìÂºÄ
            self.auto_exported_files = exported_files
            self.auto_export_ts = ts

            # UI ÊèêÁ§∫ÔºàÁä∂ÊÄÅÊ†èÔºâ
            if exported:
                self.status_label.setText(f"Â∑≤Ëá™Âä®ÂØºÂá∫ {exported} ‰∏™Ë°®Ê†ºÂà∞ÁõÆÂΩïÔºö{out_dir}")
            else:
                self.status_label.setText("Â∑≤ÂÆåÊàêÔºå‰ΩÜÊ≤°ÊúâÂèØÂØºÂá∫ÁöÑÊòéÁªÜÊï∞ÊçÆ„ÄÇ")

            # Windows ‰∏äÂèØÈÄâÔºöËá™Âä®ÊâìÂºÄÁ¨¨‰∏Ä‰∏™Êñá‰ª∂ÔºàÈúÄË¶ÅÂ∞±ÂèñÊ∂àÊ≥®ÈáäÔºâ
            # try:
            #     if sys.platform.startswith("win") and exported_files:
            #         os.startfile(exported_files[0])
            # except Exception:
            #     pass

        except Exception as e:
            try:
                QMessageBox.warning(self, "Ëá™Âä®ÂØºÂá∫Â§±Ë¥•",
                                    f"Ëá™Âä®ÂØºÂá∫Êó∂ÂèëÁîüÈîôËØØÔºö{str(e)}\nÂèØÂ∞ùËØïÊâãÂä®ÁÇπÂáª‚ÄúÂØºÂá∫‚ÄùÊàñÈáçÊñ∞Ëé∑ÂèñÊï∞ÊçÆ„ÄÇ")
            except Exception:
                pass

    def export_data(self):
        """
        ‰ªÖÊâìÂºÄ‚ÄúËé∑ÂèñÂÆåÊàêÂêéËá™Âä®ÁîüÊàê‚ÄùÁöÑ ExcelÔºå‰∏çÂÜçÈáçÊñ∞ÁîüÊàê„ÄÇ
        Ëã•Ê≤°ÊúâÂèØÊâìÂºÄÁöÑÊñá‰ª∂ÔºåÂàôÊèêÁ§∫Áî®Êà∑ÂÖàËé∑ÂèñÊï∞ÊçÆÂÆåÊàêÂêéÂÜçËØï„ÄÇ
        """
        import os, sys
        from PyQt5.QtCore import QUrl
        from PyQt5.QtGui import QDesktopServices

        files = list(getattr(self, "auto_exported_files", []) or [])
        if not files:
            QMessageBox.warning(self, "Ê≤°ÊúâÂèØÊâìÂºÄÁöÑË°®Ê†º", "Â∞öÊú™ÁîüÊàêÂØºÂá∫Êñá‰ª∂ÔºåËØ∑ÂÖà‚ÄúËé∑ÂèñÊï∞ÊçÆ‚ÄùÂπ∂Á≠âÂæÖÂÆåÊàê„ÄÇ")
            return

        # ËøáÊª§Êéâ‰∏çÂ≠òÂú®ÁöÑË∑ØÂæÑ
        existing = [f for f in files if isinstance(f, str) and os.path.exists(f)]
        if not existing:
            # Êñá‰ª∂ÂèØËÉΩË¢´ÁßªÂä®/Âà†Èô§ÔºåÊâìÂºÄ‰øùÂ≠òÁõÆÂΩïÂÖúÂ∫ï
            folder = getattr(self, "auto_export_dir", os.getcwd())
            QMessageBox.warning(self, "Êú™ÊâæÂà∞Êñá‰ª∂", "Â∑≤ÂØºÂá∫ÁöÑ Excel ÂèØËÉΩË¢´ÁßªÂä®ÊàñÂà†Èô§„ÄÇÂ∞ÜÊâìÂºÄ‰øùÂ≠òÁõÆÂΩï„ÄÇ")
            QDesktopServices.openUrl(QUrl.fromLocalFile(folder))
            return

        opened = 0
        try:
            if sys.platform.startswith("win"):
                # WindowsÔºöÈÄê‰∏™Áî®Á≥ªÁªüÈªòËÆ§Á®ãÂ∫èÊâìÂºÄ
                for f in existing:
                    os.startfile(f)
                    opened += 1
            else:
                # Èùû WindowsÔºöÂ∞ùËØïÈÄê‰∏™ÊâìÂºÄÔºõÂ§±Ë¥•ÂàôÊâìÂºÄÊâÄÂú®ÁõÆÂΩï
                for f in existing:
                    ok = QDesktopServices.openUrl(QUrl.fromLocalFile(f))
                    if ok:
                        opened += 1
                    else:
                        folder = os.path.dirname(f) or getattr(self, "auto_export_dir", os.getcwd())
                        QDesktopServices.openUrl(QUrl.fromLocalFile(folder))
                        break
        except Exception:
            # ‰ªª‰ΩïÂºÇÂ∏∏ÈÉΩÂõûÈÄÄÂà∞ÊâìÂºÄÁõÆÂΩï
            folder = os.path.dirname(existing[0]) or getattr(self, "auto_export_dir", os.getcwd())
            QDesktopServices.openUrl(QUrl.fromLocalFile(folder))

        QMessageBox.information(self, "ÊèêÁ§∫", f"Â∑≤ÊâìÂºÄ {opened} ‰∏™ÂØºÂá∫Ë°®Ê†º„ÄÇ")

    def is_expired(self):
        """Ê£ÄÊü•ÂΩìÂâçÊó∂Èó¥ÊòØÂê¶Ë∂ÖËøáÊåáÂÆöÁöÑÊà™Ê≠¢Êó∂Èó¥"""
        current_time = datetime.now()  # Ëé∑ÂèñÂΩìÂâçÁ≥ªÁªüÊó∂Èó¥
        return current_time > self.expiration_date

    def get_remaining_days(self):
        """ËÆ°ÁÆóË∑ùÁ¶ªËøáÊúüÊó∂Èó¥ËøòÊúâÂ§öÂ∞ëÂ§©"""
        current_time = datetime.now()  # Ëé∑ÂèñÂΩìÂâçÁ≥ªÁªüÊó∂Èó¥
        days_left = (self.expiration_date - current_time).days  # ËÆ°ÁÆóÂâ©‰ΩôÂ§©Êï∞
        return days_left  # ËøîÂõûÂâ©‰ΩôÁöÑÂ§©Êï∞

    def load_group_names_from_config(self):
        config = ConfigParser()
        config.read('config.ini')
        if 'GROUP_COOKIES' in config:
            return list(config['GROUP_COOKIES'].keys())
        # ÈªòËÆ§ÁªÑÔºàÈ¶ñÊ¨°ËøêË°åÊàñ config.ini Â∞ö‰∏çÂ≠òÂú®Ôºâ
        return ["ÁªøËå∂", "Â¶ñÂß¨", "Ëâ≤Êàí", "Áà±Ëâ≤Êí≠", "ÊÄßÁ¶è", "Â∞ëÂ•≥", "‰∫∫Â¶ª", "‰πêÂí™"]

    def save_config(self, port, token_dict, code):
        config = ConfigParser()

        # ‰øùÂ≠òÈÄöÁî®ÈÖçÁΩÆ
        config['COMMON'] = {
            'Port': port,
            'Code': code
        }

        # ‰øùÂ≠òÂ∞èÁªÑCookieÂà∞Áªü‰∏ÄÂàÜÂå∫
        config['GROUP_COOKIES'] = {group: token for group, token in token_dict.items()}

        with open('config.ini', 'w') as configfile:
            config.write(configfile)

    def load_config(self):
        config = ConfigParser()
        config.read('config.ini')

        if 'COMMON' in config:
            self.port_input.setText(config['COMMON'].get('Port', ''))
            self.code_input.setText(config['COMMON'].get('Code', ''))

        if 'GROUP_COOKIES' in config:
            for group, token in config['GROUP_COOKIES'].items():
                if group in self.token_inputs:
                    self.token_inputs[group].setText(token)

    def manual_save(self):  # ÊâãÂä®‰øùÂ≠òÈÖçÁΩÆ
        port = self.port_input.text()
        code = self.code_input.text()

        token_dict = {}
        missing_groups = []

        for group, input_field in self.token_inputs.items():
            token = input_field.text()
            if token:
                token_dict[group] = token
                input_field.setStyleSheet("")  # Ê∏ÖÈô§Á∫¢Ê°Ü
            else:
                missing_groups.append(group)
                input_field.setStyleSheet("border: 1px solid red;")

        if not port or not code:
            QMessageBox.warning(self, "Ë≠¶Âëä", "ËØ∑Â°´ÂÜôÂÆåÊï¥ÁöÑÈÖçÁΩÆÔºàportÂíåcodeÔºâÔºÅ")
            return

        self.save_config(port, token_dict, code)

        if missing_groups:
            missing_text = "„ÄÅ".join(missing_groups)
            QMessageBox.information(self, "ÈÉ®ÂàÜ‰øùÂ≠òÊàêÂäü", f"ÈÖçÁΩÆÂ∑≤‰øùÂ≠òÔºå‰ΩÜ‰ª•‰∏ãÂ∞èÁªÑÊú™Â°´ÂÜô CookieÔºö{missing_text}")
        else:
            QMessageBox.information(self, "ÊèêÁ§∫", "‚úÖ ÈÖçÁΩÆÂ∑≤ÂÆåÊï¥‰øùÂ≠òÔºÅ")


if __name__ == "__main__":
    app = QApplication(sys.argv)
    window = UserWindow()
    window.show()
    sys.exit(app.exec_())
